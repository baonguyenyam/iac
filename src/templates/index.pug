										p.last-character 8845454548744
				.col-3.bg-yellow.d-flex.align-items-center
					article.p-3.color-black
						.row.mb-2
							.col-6 
								h3.info-text Qty.
								.number-text 193
							.col-6 
								h3.info-text In build
								.number-text 56
						.row.secondary-text
							.col-6 
								h3.info-text Process
								.number-text 56
									span.percent %
							.col-6 
								h3.info-text Est.
								.number-text 1h36
