extends _layout/layout

//- Default value
block variables
	- var title = 'Home'
	- var description = 'Description for home page'
	- var bodyclass = 'home'
	- var href = "/index.html"

//- body
block body
	section.in-process.in-processing
		.container-fullwidth
			.row.no-gutters
				.col
					article.p-2.bg-sen.color-main
						.row
							.col-auto
								h4.info-text Schedule
								.desc-text 59813658
							.col 
								h4.info-text Name
								.desc-text Denali SWB Premium

					article.px-3.pt-3.pb-0
						.group-items
							.item
								figure 
									.icons
										img.svg(src="./img/lwb.svg", alt="")
									figcaption
										h3.last-title LWB Chevy
										p.last-character TGA4587989991600
							.item
								figure 
									.icons
										img.svg(src="./img/grill.svg", alt="")
									figcaption
										h3.last-title Speaker Grill Jet Black
										p.last-character EM-8979KD15688
							.item
								figure 
									.icons
										img.svg(src="./img/harness.svg", alt="")
									figcaption
										h3.last-title Wire Harness 
										p.last-character 879445636
							.item
								figure 
									.icons
										img.svg(src="./img/speaker.svg", alt="")
									figcaption
										h3.last-title Speaker
										p.last-character Denali S124
							.item
								figure 
									.icons
										img.svg(src="./img/deco-denali.svg", alt="")
